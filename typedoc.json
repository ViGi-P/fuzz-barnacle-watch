{
	"out": "./docs",
	"readme": "none",
	"includes": "./src",
	"exclude": [
		"**/setupTests.ts",
		"**/__tests__/**/*",
		"**/__fixtures__/**/*",
		"**/constants/*",
		"**/mappers/*",
		"**/utils/*"
	],
	"excludeExternals": true,
	"excludePrivate": true,
	"entryPointStrategy": "expand",
	"entryPoints": ["./src"]
}
